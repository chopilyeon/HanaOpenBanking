<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="dao.TransactionDAO">
	<resultMap type="transactionVO" id="transactionMap">
   	    <result column ="name" property="insertBank"/>
   	    <result column ="password" property="withrawalAccount"/>
		<result column ="identity" property="depositRecipient"/>
		<result column ="id" property="depositAccount"/>
		<result column ="gender" property="remittance"/>
		<result column ="phone_number" property="tranDate"/>
	</resultMap>
	
	<select>
		select 
			B.bank_name as withrawal_bank_name, deposit_bank_name, tran_num, deposit_account_num,withrawal_account_num,deposit_name,withrawal_name,deposit_bank_code,withrawal_bank_code,tran_amount,tran_date	
		from 
		(select B.bank_name as deposit_bank_name,tran_num,
		deposit_account_num,withrawal_account_num,deposit_name,withrawal_name,deposit_bank_code,withrawal_bank_code,tran_amount,tran_date		
		from transaction_history A, bank_code B		
		where A.deposit_bank_code = b.bank_code) A, bank_code B
		where A.withrawal_bank_code = B.bank_code
	
	
	
	
	</select>





	
</mapper>